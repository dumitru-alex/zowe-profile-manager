{
  "name": "sys-mon",
  "productName": "System-Monitor",
  "version": "1.0.0",
  "description": "System Monitor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"SysMon\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "keywords": [
    "Electron"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dumitru-alex/zowe-profile-manager.git"
  },
  "author": "Alex Dumitru",
  "license": "MIT",
  "devDependencies": {
    "electron": "^10.1.4",
    "electron-packager": "^15.1.0",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "electron-log": "^4.2.4",
    "node-os-utils": "^1.3.2"
  }
}
