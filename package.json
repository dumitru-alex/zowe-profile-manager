{
	"name": "zowe-profile-manager",
	"displayName": "zowe-profile-manager",
	"description": "Zowe CLI Profile Manager",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:zowe-profile-manager.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "zowe-profile-manager.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^7.10.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
